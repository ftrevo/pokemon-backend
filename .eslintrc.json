{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      2
    ],
    "no-console": 1
    // , "require-await": 2
  }
}